  <script>
    // $('#stickymenu_bottom_mobile').hide()  

    document.getElementById('pay-button').onclick = function (e) {
      $('#coin_offer').modal('hide');
      let amount = $('input[name="offerName"]:checked').val()
      let offerId = $('input[name="offerName"]:checked').attr('id');
      setValues(offerId, amount)
    }

    function payment(e) {
      let offerId = document.getElementById('offerId').value
      let amount = $('#amount').val()
      let data = {
        offerId,
        amount
      }
      axios({
        method: 'post',
        url: '/candidate/payment',
        data,
        headers: {
          'x-auth': localStorage.getItem('token')
        }
      }).then((res) => {
        let options = {
          "key": '<%= process.env.MIPIE_RAZORPAY_KEY %>',
          "amount": res.data.order.amount,
          "currency": res.data.order.currency,
          "name": "MiPie",
          "description": "",
          "image": "/images/logo/logo.png",
          "order_id": res.data.order.id,
          "handler": function (response) {
            let data = {
              paymentId: response.razorpay_payment_id,
              orderId: response.razorpay_order_id,
              _candidate: res.data.candidate._id,
              _offer: offerId,
              amount
            }
            axios({
              method: 'post',
              url: '/candidate/paymentStatus',
              data,
              headers: {
                'x-auth': localStorage.getItem('token')
              }
            }).then(res => {
              window.location.reload()
            }).catch(
              (error) => {
                console.log(error.message)
              })
          },
          "prefill": {
            "name": res.data.candidate.name,
            "email": res.data.candidate.email,
            "contact": res.data.candidate.mobile
          },
          "theme": {
            "color": "#FC2B5A"
          }
        };
        let rzp1 = new Razorpay(options);
        rzp1.open();
        e.preventDefault();
      }).catch(
        err => console.log(err.message)
      )
    }
    $('.apply-div-field').click(() => {
      $('#apply-field').val('true');
    })
    $('.call-div-field').click(() => {
      $('#apply-field').val('false')
    })

    function applyJob(id) {
      $('#apply-btn').addClass('disabled');
      document.getElementById('apply-btn').setAttribute('disabled', true)
      axios({
        method: 'post',
        url: `/candidate/job/${id}/apply`,
        headers: {
          'x-auth': localStorage.getItem('token')
        }
      })
        .then(result => {
          $('#apply-btn').removeClass('disabled');
          document.getElementById('apply-btn').setAttribute('disabled', false)
          if ($('#apply-field').val() == 'false') {
            $('#apply').modal('hide');
            if ($('#for-mobile').val() == 'true') {
              $('#apply-div-btn-mob').html(`<i class="la la-paper-plane ml-3"></i>Applied / प्रयुक्त`)
              $('#apply-div-btn-mob').addClass('disabled-button')
              $('#has-applied').val('true');
              window.location.href = "tel://" + '<%=mobileNumber%>'
            } else {
              $('#apply-div-btn').html(`<i class="la la-paper-plane ml-3"></i>Applied /प्रयुक्त`)
              $('#apply-div-btn').addClass('disabled-button')
              $('.call-div-field').removeAttr('data-toggle')
              $('.call-div-field').removeAttr('data-target')
              $('.call-div-field').attr('href', "tel://" + '<%=mobileNumber%>')
              window.location.href = "tel://" + '<%=mobileNumber%>'
            }
          } else {
            $('#apply').modal('hide');
            $('#registerApply').modal('show');
          }
        }).catch(err => {
          console.log(err.message)
          window.location.reload()
        })
    }

    /* register for intervies */
    function registerForInterview(id) {
      $('#apply-btn').addClass('disabled');
      document.getElementById('apply-btn').setAttribute('disabled', true)
      axios({
        method: 'post',
        url: `/candidate/job/${id}/registerInterviews`,
        headers: {
          'x-auth': localStorage.getItem('token')
        }
      })
        .then(result => {
          $('#apply-btn').removeClass('disabled');
          document.getElementById('apply-btn').setAttribute('disabled', false)
          if ($('#apply-field').val() == 'false') {
            $('#apply').modal('hide');
            if ($('#for-mobile').val() == 'true') {
              $('#apply-div-btn-mob').html(`<i class="la la-paper-plane ml-3"></i>Registered for Interview `)
              $('#apply-div-btn-mob').addClass('disabled-button')
              $('#has-applied').val('true');
              window.location.href = "tel://" + '<%=mobileNumber%>'
            } else {
              $('#apply-div-btn').html(`<i class="la la-paper-plane ml-3"></i>Registered for Interview`)
              $('#apply-div-btn').addClass('disabled-button')
              $('.call-div-field').removeAttr('data-toggle')
              $('.call-div-field').removeAttr('data-target')
              $('.call-div-field').attr('href', "tel://" + '<%=mobileNumber%>')
              window.location.href = "tel://" + '<%=mobileNumber%>'
            }
          } else {
            $('#registerApply').modal('hide');
            $('#afterApply').modal('show');
          }

          // window.location.reload()
        }).catch(err => {
          console.log(err.message)
          window.location.reload()
        })
    }

    function getOffers() {
      axios({
        method: 'get',
        url: `/candidate/getCoinOffers`,
        headers: {
          'x-auth': localStorage.getItem('token')
        }
      })
        .then(res => {
          let offers = document.getElementById('offers');
          let html = ''
          for (let i of res.data) {
            html +=
              `<div class="row inner-border my-2 text-white popup-bg py-1" ><div class="col-9 pr-0">${i.displayOffer}</div><div class="col-3 text-left"><span>  <input type="radio" id="${i._id}" name="offerName" value="${i.payAmount.$numberDecimal}" class="radio-size "> </span></div></div>`
          }
          offers.innerHTML = html
          $('.radio-size').first().prop("checked", true)
        })
    }

    function setValues(offerId, amount) {
      let offer = document.getElementById('offerId')
      let voucherAmount = document.getElementById('amount')
      let offerAmount = document.getElementById('offerAmount')
      offer.value = offerId;
      voucherAmount.value = amount
      offerAmount.value = amount
      document.getElementById('applyButton').innerHTML = `Pay / भुगतान करें ₹ ${amount}`
    }

    function applyVouchers(e) {
      let code = document.getElementById('voucher').value
      if (code.trim() == '') {
        return payment(e)
      }
      let amount = document.getElementById('offerAmount').value
      let offer = document.getElementById('offerId').value
      let data = {
        amount,
        code,
        offerId: offer
      }
      axios({
        method: 'put',
        url: '/candidate/applyVoucher',
        data,
        headers: {
          'x-auth': localStorage.getItem('token')
        }
      })
        .then((res) => {
          if (res.data.status == true && res.data.amount > 0) {
            $('#errorMsg').hide();
            $('#errorMsg').text('');
            $('#successMsg').show();
            $('#successMsg').text(res.data.message);
            document.getElementById('amount').value = res.data.amount
            document.getElementById('paymentVoucher').value = code
            document.getElementById('applyButton').innerHTML = `Pay / भुगतान करें ₹ ${res.data.amount}`
            return payment(e)
          } else if (res.data.status == true && res.data.amount == 0) {
            $('#errorMsg').hide();
            $('#errorMsg').text('');
            $('#successMsg').show();
            $('#successMsg').text(res.data.message);
            window.location.reload();
          } else {
            $('#successMsg').hide();
            $('#successMsg').text('');
            $('#errorMsg').show();
            $('#errorMsg').text(res.data.message);
            document.getElementById('voucher').value = ''
            let offerAmount = document.getElementById('offerAmount').value
            document.getElementById('applyButton').innerHTML = `Pay / भुगतान करें ₹ ${offerAmount}`
          }

        })
        .catch((err) => {
          console.log(err)
        })
    }
