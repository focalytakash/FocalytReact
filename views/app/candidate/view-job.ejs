    function sendReview(_id) {
      let data = {};
      $('#review-form').serializeArray().forEach(ele => {
        if (ele.name !== "") {
          data[ele.name] = ele.value;
        }
      });

      axios({
        method: 'post',
        url: `/candidate/review/${_id}`,
        data,
        headers: {
          'x-auth': localStorage.getItem('token')
        }
      })
        .then((res) => {
          window.location.reload()
        })
        .catch((err) => {
