                                                                    <!-- <td>
                                                                                        <a class="btn btn-success"
                                                                                            href="/college/candidate/createResume/<%= l._id %>">Create
                                                                                            Resume</a>
                                                                                    </td> -->
                                                                    </a>

                                                                    <% if(l.status) { %>

                                                                    <% } else { %>

                                                                    </label>
                                                                    <% } %>
                                                    </div>
                                                    </td>
