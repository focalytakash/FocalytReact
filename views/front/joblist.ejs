                        class="img-fluid p-1"> </div> -->
                    <div class="job-head-info">
                      <h4 class="text-capitalize">
                        <%=recentJob.title%>
                      </h4>
                      <span class="text-capitalize">
                        <%= recentJob.displayCompanyName?recentJob.displayCompanyName : recentJob._company?.name%>
                      </span>
                    </div>
                  </div>
                  <div class="job-overview mt-2">
                    <ul class="mb-xl-0 mb-lg-2 mb-md-2 mb-sm-0 mb-0">
                      <!-- <li><i class="la la-thumb-tack"></i>
                        <h3 class="text-capitalize">
                          <%=recentJob.title%>
                        </h3><span class="text-capitalize">
                          Career Level / करियर स्तर
                        </span>
                      </li>
                      <li><i class="la la-puzzle-piece"></i>
                        <h3>
                          <%=recentJob._industry?.name%>
                        </h3><span class="text-capitalize">
                          Industry / उद्योग
                        </span>
                      </li>
                      <li><i class="la la-puzzle-piece"></i>
                        <h3 class="text-capitalize">
                          <%=recentJob._jobCategory?.name%>
                        </h3><span class="text-capitalize">
