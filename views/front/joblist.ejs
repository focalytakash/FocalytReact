          </div>
          <div class="row">
            <div class="col-xl-12">
              <ul id="portfolio-flters" class="d-flex justify-content-center">
                <li class="">
                  <select class="form-control" name="qualification" id="qualification">
                    <option value="">Select Qualification</option>
                    <% allQualification.forEach((i)=> { %> <% if(i._id==data.qualification){ %>
                        <option selected value="<%= i._id %>" class="text-capitalize">
                          <%= i.name %>
                        </option>
                        <% } else { %>
                          <option value="<%= i._id %>" class="text-capitalize">
                            <%= i.name %>
                          </option>
                          <% } %>
                            <% }); %>
                  </select>
                </li>
                <li class="">
                  <select class="form-control" name="experience" id="experience">
                    <option value="">Select Experience</option>
                    <option value="0" <% if (data.experience=='0' ) { %> selected <% }%>>0</option>
                    <% for(let i=1; i<16; i++) {%>
                      <option value="<%= i %>" <% if (data.experience==i ) { %> selected <% }%> ><%= i %>
                      </option>
                      <% } %>
