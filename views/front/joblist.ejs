                          <option value="<%= i._id %>" class="text-capitalize">
                            <%= i.name %>
                          </option>
                          <% } %>
                            <% }); %>
                  </select>
                </li>
                <li class="">
                  <select class="form-control" id="jobType" name="jobType">
                    <option value="">Select JobType</option>
                    <option value="Part Time" <% if(data.jobType=="Part Time" ) {%> selected <% } %>>Part
                        Time</option>
                    <option value="Full Time" <% if(data.jobType=="Full Time" ) {%> selected <% } %>>Full
                        Time</option>
                  </select>
                </li>
                <li class="">
                  <select class="form-control" id="state" name="state">
                    <option value="">Select State</option>
                    <% allStates.forEach((i)=> { %> <% if(i._id==data.state){ %>
                        <option selected value="<%= i._id %>" class="text-capitalize">
                          <%= i.name %>
                        </option>
                        <% } else { %>
                          <option value="<%= i._id %>" class="text-capitalize">
                            <%= i.name %>
                          </option>
                          <% } %>
                            <% }); %>
                  </select>
                </li>
                <li class="">
                  <select class="form-control" id="Salary" name="Salary">
                    <option value="">Select Salary</option>
                    <option value="5000" <% if(data.Salary=="5000" ) {%> selected <% } %>>5000</option>
                    <option value="10000" <% if(data.Salary=="10000" ) {%> selected <% } %>>10000
                    </option>
                    <option value="15000" <% if(data.Salary=="15000" ) {%> selected <% } %>>15000
                    </option>
                    <option value="20000" <% if(data.Salary=="20000" ) {%> selected <% } %>>20000
                    </option>
                    <option value="30000" <% if(data.Salary=="30000" ) {%> selected <% } %>>30000
                    </option>
                    <option value="40000" <% if(data.Salary=="40000" ) {%> selected <% } %>>40000
                    </option>
                    <option value="50000" <% if(data.Salary=="50000" ) {%> selected <% } %>>50000
                    </option>
                    <option value="70000" <% if(data.Salary=="70000" ) {%> selected <% } %>>70000
                    </option>
                    <option value="80000" <% if(data.Salary=="80000" ) {%> selected <% } %>>80000+
