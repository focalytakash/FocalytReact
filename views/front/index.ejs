<script>
  
  document.addEventListener('DOMContentLoaded', function() {
    const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                // Check if we're on mobile
                const isMobile = window.innerWidth <= 768;
                const baseDelay = isMobile ? 200 : 300; // Faster initial delay on mobile
                const itemDelay = isMobile ? 200 : 300; // Faster between-item delay on mobile
                
                // Animate title first
                const title = entry.target.querySelector('.primary-gradient');
                if (title) {
                    setTimeout(() => {
                        title.classList.add('animate');
                    }, baseDelay);
                }
                
                // Animate items with appropriate delays
                const items = entry.target.querySelectorAll('.tech_area_img');
                items.forEach((item, index) => {
                    const row = Math.floor(index / 2); // For 2-column mobile grid
                    const delay = baseDelay + ((isMobile ? row : index) * itemDelay);
                    
                    setTimeout(() => {
                        item.classList.add('animate');
                    }, delay + 300);
                });
            } else {
                if (entry.intersectionRatio <= 0.1) {
                    const title = entry.target.querySelector('.primary-gradient');
                    const items = entry.target.querySelectorAll('.tech_area_img');
                    
                    // Smoother reset for mobile
                    if (title) title.classList.remove('animate');
                    items.forEach((item, index) => {
                        setTimeout(() => {
                            item.classList.remove('animate');
                        }, index * (window.innerWidth <= 768 ? 100 : 150));
                    });
                }
            }
        });
    }, {
        threshold: [0.1, 0.5],
        rootMargin: '20px' // Smaller margin for mobile
    });

    // Start observing the section
    const section = document.getElementById('new');
    if (section) {
        observer.observe(section);
        
        // Handle resize events for responsive behavior
        window.addEventListener('resize', () => {
            const items = section.querySelectorAll('.tech_area_img');
            items.forEach(item => item.classList.remove('animate'));
            setTimeout(() => {
                if (isElementInViewport(section)) {
                    items.forEach((item, index) => {
                        setTimeout(() => {
                            item.classList.add('animate');
                        }, (window.innerWidth <= 768 ? 200 : 300) * (index + 1));
                    });
                }
            }, 100);
        });
    }
});

// Helper function to check if element is in viewport
function isElementInViewport(el) {
    const rect = el.getBoundingClientRect();
    return (
        rect.top >= 0 &&
        rect.left >= 0 &&
        rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
        rect.right <= (window.innerWidth || document.documentElement.clientWidth)
    );
}

