document.addEventListener('DOMContentLoaded', function() {
    // Intersection Observer for initial animation
    const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                // Animate title first
                const title = entry.target.querySelector('.primary-gradient');
                if (title) {
                    setTimeout(() => {
                        title.classList.add('animate');
                    }, 100);
                }
                
                // Animate items with pop-in effect
                const items = entry.target.querySelectorAll('.tech_area_img');
                items.forEach((item, index) => {
                    setTimeout(() => {
                        item.classList.add('animate');
                    }, 200 + (index * 150));
                });
            } else {
                // Reset animations when out of view
                const title = entry.target.querySelector('.primary-gradient');
                const items = entry.target.querySelectorAll('.tech_area_img');
                
                if (title) title.classList.remove('animate');
                items.forEach(item => {
                    item.classList.remove('animate');
                });
            }
        });
    }, {
        threshold: 0.2,
        rootMargin: '20px'
    });

    // Click handler for mobile devices
    if (window.innerWidth <= 768) {
        const section = document.getElementById('mobile-new');
        if (section) {
            observer.observe(section);
            
            // Add click handlers to each tech item
            const techItems = section.querySelectorAll('.tech_area_img');
            techItems.forEach(item => {
                item.addEventListener('click', function() {
                    // Remove clicked class from all items
                    techItems.forEach(otherItem => {
                        otherItem.classList.remove('clicked');
                    });
                    
                    // Add clicked class to current item
                    this.classList.add('clicked');
                    
                    // Remove clicked class after animation completes
                    setTimeout(() => {
                        this.classList.remove('clicked');
                    }, 800);
                });
            });
        }
    }

    // Handle orientation change
    window.addEventListener('orientationchange', () => {
        if (window.innerWidth <= 768) {
            const section = document.getElementById('mobile-new');
            if (section) {
                const items = section.querySelectorAll('.tech_area_img');
                items.forEach(item => item.classList.remove('animate'));
                setTimeout(() => {
                    items.forEach((item, index) => {
                        setTimeout(() => {
                            item.classList.add('animate');
                        }, 200 * index);
                    });
                }, 100);
            }
        }
    });
});
