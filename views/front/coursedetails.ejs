<%- storageScript %>

            <script>
                const videoModal = document.getElementById('videoModal')
                if (videoModal) {
                    videoModal.addEventListener('show.bs.modal', event => {
                        // Button that triggered the modal
                        const button = event.relatedTarget
                        // Extract info from data-bs-* attributes
                        const link = button.getAttribute('data-bs-link')

                        // Update the modal's content.
                        const modalBody = videoModal.querySelector('.modal-body #vodeoElement')

                        modalBody.src = link
                        document.getElementById('courseVid').load()
                    })
                    videoModal.addEventListener('hide.bs.modal', event => {
                        document.getElementById('courseVid').pause()
                    })
                }
            </script>