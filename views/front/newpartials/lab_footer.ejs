<script>
  $(document).ready(function () {
    const candidateImages = $("#blog--images .happy_candidate_images");

    if (candidateImages.children().length > 1) {
        candidateImages.slick({
            dots: true,
            infinite: true,
            slidesToShow: 1,
            slidesToScroll: 1,
            arrows: true, // Navigation arrows
            autoplay: false,
            autoplaySpeed: 10000,
            responsive: [
                {
                    breakpoint: 1366,
                    settings: {
                        slidesToShow: 1,
                    },
                },
                {
                    breakpoint: 768,
                    settings: {
                        slidesToShow: 1,
                    },
                },
            ],
        });
    } else if (candidateImages.children().length === 1) {
        candidateImages.children().css({
            width: "100%",
            display: "block",
        });
        candidateImages.css({
            display: "flex",
            justifyContent: "center",
        });
    }
});

</script>
<script>
  $(document).ready(function () {
    const candidateImages = $("#blog--images");
    const imageCount = candidateImages.find(".happy_candidate_images").children().length;

    if (imageCount > 1) {
      // Initialize Slick Slider for multiple images
      candidateImages.find(".happy_candidate_images").slick({
        dots: true,
        infinite: true,
        slidesToShow: 1,
        slidesToScroll: 1,
        arrows: true, // Add arrows for navigation
        autoplay: false,
        autoplaySpeed: 2000,
        responsive: [
          {
            breakpoint: 1366,
            settings: {
              slidesToShow: 1,
            },
          },
          {
            breakpoint: 768,
            settings: {
              slidesToShow: 1,
            },
          },
        ],
      });
    } else if (imageCount === 1) {
      // For a single image, ensure it displays properly
      candidateImages.find(".happy_candidate_images").children().css({
        width: "100%",
        display: "block",
      });
      candidateImages.find(".happy_candidate_images").css({
        display: "flex",
        justifyContent: "center",
      });
    }
  });
</script>

