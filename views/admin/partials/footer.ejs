  <script>
    document.addEventListener('DOMContentLoaded', function() {
    const dropdownTrigger = document.querySelector('.dropdown-trigger');
    const dropdownContainer = dropdownTrigger.closest('.dropdown-container');
  
    // Toggle dropdown
    dropdownTrigger.addEventListener('click', function(e) {
      e.stopPropagation();
      dropdownContainer.classList.toggle('active');
    });
  
    // Close dropdown when clicking outside
    document.addEventListener('click', function() {
      dropdownContainer.classList.remove('active');
    });
  
    // Prevent dropdown from closing when clicking inside
    dropdownContainer.addEventListener('click', function(e) {
      e.stopPropagation();
    });
  
    // Optional: Add event listeners for edit and delete actions
    const editItem = dropdownContainer.querySelector('.dropdown-item:first-child');
    const deleteItem = dropdownContainer.querySelector('.dropdown-item:last-child');
  
    editItem.addEventListener('click', function() {
      // Add your edit logic here
      console.log('Edit clicked');
      dropdownContainer.classList.remove('active');
    });
  
    deleteItem.addEventListener('click', function() {
      // Add your delete logic here
      console.log('Delete clicked');
      dropdownContainer.classList.remove('active');
    });
  });
  </script>
