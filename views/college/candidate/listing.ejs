	</div>
	</section>
	<% include ../partials/footer %>
		<script>
			function myOnOffFunc(id, status, model) {
				// eslint-disable-line
				let myStatus = true;
				if (status === true || status === "true") {
					myStatus = false;
				}
				$.ajax({
					type: "POST",
					url: "/changestatus",
					data: {
						id,
						status: myStatus,
						model,
					},
					success: () => location.reload(),
				});
			}
			function deleteRecord(id, status, model) {
				// eslint-disable-line
				let myStatus = true;
				if (status === true || status === "true") {
					myStatus = false;
				}
				var x = confirm("Are you sure you want to delete?");
				if (x) {
					$.ajax({
						type: "POST",
						url: "/deleteRecord",
						data: {
							id,
							status: myStatus,
							model,
						},
						success: () => (window.location.href = "/college/candidate"),
					});
				}
			}
		</script>
		<script>
			!(function () {
				"use strict";
				Array.isArray ||
					(Array.isArray = function (r) {
						return "[object Array]" === Object.prototype.toString.call(r);
					});
				var r = {
					get: function () {
						var r = window.location.search,
							t = {};
						return "" === r
							? t
							: ((r = r.slice(1)),
								(r = r.split("&")),
								r.map(function (r) {
									var i, o;
									(r = r.split("=")),
										(i = r[0]),
										(o = r[1]),
										t[i]
											? (Array.isArray(t[i]) || (t[i] = [t[i]]),
												t[i].push(o))
											: (t[i] = o);
								}),
								t);
					},
				};
				if (window) {
					if (window.qs)
						throw new Error(
							"Error bootstrapping qs: window.qs already set."
						);
					window.qs = r;
				}
			})();
		</script>
		<script>
			document.addEventListener("DOMContentLoaded", () => {
				const pageAnchors = document.querySelectorAll("a.pageAnchor");
				const { origin, pathname, search } = window.location;
				const obj = window.qs.get(search);
				const keyArr = Object.keys(obj).filter((x) => !!obj[x]);
				let url = `${origin}${pathname}?`;
				keyArr.forEach((x) => {
					if (x !== "page") {
						url += `${x}=${obj[x]}&`;
					}
				});
				pageAnchors.forEach((el) => {
					const n = el.getAttribute("href");
					el.setAttribute("href", `${url}page=${n}`);
				});
			});
		</script>
		</div>